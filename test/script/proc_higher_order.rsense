type-inference --test=Proc.HigherOrder1 --should-be=Fixnum,Bignum
def set(&block)
  $block = block
end
set {|a| a + 1}
$block.call(1)_|_
EOF

type-inference --test=Proc.HigherOrder2 --should-be=Fixnum,Bignum
def adder(a)
  proc {|b| a + b }
end
adder(1).call(2)_|_
EOF

